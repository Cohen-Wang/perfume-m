{
  "name": "vite-cli",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "eslint:init": "eslint --init",
    "eslint:lint": "eslint \"src/**/*.{vue,js,ts}\"",
    "eslint:fix": "eslint \"src/**/*.{jvue,js,ts}\" --fix",
    "prettier:fix": "prettier --config .prettierrc.js \"src/**/*.{vue,js,ts}\" --write",
    "stylelint:lint": "stylelint **/*.{vue,css,sass,scss}",
    "stylelint:fix": "stylelint **/*.{vue,css,sass,scss} --fix",
    "commitlint": "commitlint --config commitlint.config.js -e -V"
  },
  "dependencies": {
    "naive-ui": "^2.33.5",
    "pinia": "^2.0.23",
    "vue": "^3.2.41",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@types/eslint": "^8.4.10",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.7.0",
    "husky": "^8.0.0",
    "postcss": "^8.4.18",
    "postcss-html": "^1.5.0",
    "prettier": "^2.7.1",
    "sass": "^1.56.0",
    "sass-loader": "^13.1.0",
    "stylelint": "^14.14.1",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "typescript": "^4.8.4",
    "vite": "^3.2.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-stylelint": "^3.0.8",
    "vue-tsc": "^1.0.9"
  },
  "lint-staged": {
    "*.{vue,js,jsx,ts,tsx}": [
      "npm run eslint:lint",
      "npm run prettier:fix"
    ],
    "*.{vue,css,sass,scss}": [
      "npm run stylelint:fix"
    ]
  }
}
